{
  "collectionName": "components_sections_page_header_with_images",
  "info": {
    "displayName": "Page header with image"
  },
  "options": {},
  "attributes": {
    "heading": {
      "type": "text"
    },
    "text": {
      "type": "text",
      "required": true
    },
    "picture": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "overlayColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "overlayOpacity": {
      "type": "decimal",
      "max": 100,
      "min": 0
    },
    "isImageFixed": {
      "type": "boolean",
      "default": false
    }
  }
}
